@media (max-width:420px) {


    /* Hero */
    .carousel-caption h1 {
        font-size: 1.2rem;
    }

    .carousel-caption h5 {
        font-size: 0.725rem;
    }


    /* About */
    #box-2 h1 {
        font-size: 2rem;
    }

    .box-2 h6 {
        font-size: 2rem;
        margin-top: 1rem;
    }


    /* hotel cards */
    .card-title-room {
        text-transform: capitalize;
        font-size: 1.3rem;
        font-weight: 500;
    }

    .card-footer-room {
        text-transform: capitalize;
        font-size: 1rem;
    }
}

@media (max-width: 999px) {


    /* Header */
    .head-btn {
        background-color: none;
        padding: 20px 0px;
        padding-left: 20px;
        clip-path: none;
        display: block;
    }

    #head-nav ul::after {
        display: none;
    }

    .head-btn a {
        margin-left: 0px;
        font-size: 1.2rem;
    }


    /* Hero */
    .carousel-caption h1 {
        font-size: 1.8rem;
    }

    .carousel-caption h5 {
        font-size: 0.925rem;
    }
}






