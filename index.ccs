@import url('https://fonts.googleapis.com/css2?family=Gabarito:wght@400;500;600;700;800;900&family=Great+Vibes&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Gilda+Display&family=Lato:wght@400;700&family=League+Spartan:wght@400;500;600;700&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Gilda+Display&family=Lato:wght@400;700&family=League+Spartan:wght@400;500;600;700&family=Lobster+Two:ital,wght@0,400;0,700;1,400;1,700&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');

body {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    background-color: rgb(255, 255, 255);
    color: rgb(0, 0, 0);
    font-family: 'Gilda Display', serif;
    scroll-behavior: smooth;
    transition: all 0.3s ease-in-out;
}

:root {
    --first-font-family: 'Gilda Display', serif;
    --second-font-family: 'Great Vibes', cursive;
    --secondr-color: #9b804e;
}

/* Universal */
#headline {
    margin-top: 2.5rem;
}

.content-heading h5 {
    font-size: 0.625rem;
    letter-spacing: 5px;
    font-weight: bold;
    color: #00000091;
    text-align: center;
}

.content-heading-1 h1 {
    text-align: center;
    color: #9b804e;
    font-family: var(--second-font-family);
    font-size: 3rem;
}

/* NAVIGATION BAR */
.bgcolor.scrolled {
    background-color: rgba(0, 0, 0, 0.728);
    padding: 8px 0px 10px 0;

}
.navbar{
    z-index: 99999;
}
.head-btn {
    background-color: var(--secondr-color);
    padding: 5px 20px;
    padding-left: 25px;
    clip-path: polygon(11% 0, 100% 0, 100% 100%, 0% 100%);
    display: flex;
    justify-content: center;
    align-items: center;
    transition: all 0.4s ease-in-out;
    border: 1px solid var(--secondr-color);
}

.head-btn:hover {
    background-color: transparent;
    color: var(--secondr-color);
}

.head-btn a {
    text-decoration: none;
    margin-left: 6px;
    font-size: 0.9rem;
    font-weight: bold;
    transition: all 0.4s ease-in-out;
}

#head-nav {
    position: relative;
    transition: all 0.4s ease-in-out;
}

#head-nav ul li {
    font-size: 1.1rem;
    margin-left: 20px;
    font-weight: bold;

}

#head-nav ul li a {

    transition: all 0.4s ease-in-out;
}

.active-1 {
    color: var(--secondr-color);
}

#head-nav ul li a:hover {
    color: var(--secondr-color);
}

#head-nav ul::after {
    content: '';
    width: 80%;
    position: absolute;
    height: 2px;
    background: var(--secondr-color);
    background: linear-gradient(90deg, rgba(255, 0, 0, 0) 0%, #937a4d 85%, rgb(112, 90, 51) 100%);
    left: 20%;
}



/* HERO SECTION */
#hero {
    margin-top: -1rem;
}

.carousel-item img {
    filter: brightness(40%);
}

.carousel-caption h1 {
    font-weight: bold;
    font-size: 3.5rem;
    color: white;
    transition: all 0.4s ease-in-out;
}

.carousel-caption h5 {
    color: var(--secondr-color);
    transition: all 0.4s ease-in-out;
}

.carousel-caption button {
    background-color: transparent;
    border: 2px solid var(--secondr-color);
    font-weight: bold;
    color: var(--secondr-color);
    font-size: 1.2rem;
    transition: all 0.4s ease-in-out;
}

.carousel-caption button:hover {
    background-color: var(--secondr-color);
    color: white;
    border-color: var(--secondr-color);
}

/* HERO SECTION FINSH */

/* BOOK SECTION  */
.btn-bd {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 2.7rem;
}

.btn-bd button {
    padding: 8px 20px;
    background-color: var(--secondr-color);
    border: none;
    border-radius: 5px;
    font-size: 1.1rem;
    font-weight: bold;
}

.row h3 {
    font-size: 2rem;
    text-transform: capitalize;
}

/* hotel-index */
#our-featured-main-container {
    justify-content: center;
}

#our-featured {
    padding: 6rem 4rem 2rem 4rem;
}

#our-featured-b1 {}

#our-featured-b2 {
    display: flex;
    justify-content: center;
    align-items: center;
}

.our-featured-b2-img {
    width: 30rem;
    height: 25rem;
    overflow: hidden;
}

.our-featured-b2-img img {
    max-width: 100%;
    max-height: 100%;
    object-fit: scale-down;
}

#of-b1-inner-b1 h6 {
    font-size: 1rem;
    color: #9b804e;
    margin-bottom: 0.625rem;
    font-weight: bold;
    letter-spacing: 2.5px;
}

#of-b1-inner-b1 h1 {
    font-size: 3.5rem;
    margin-bottom: 1.5rem;
}

#of-b1-inner-b2 {
    display: flex;
    align-items: center;
    gap: 25px;
    margin-top: 3rem;
}

.of-b1-inner-b2-phnlogo {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 1px solid #9b804e;
}

.of-b1-inner-b2-phnlogo i {
    font-size: 25px;
    color: #9b804e;
}

.of-b1-inner-b2-phn span {
    font-size: 1.5rem;
}

.smart-key-0-img {
    border: 1px solid #9b804e;
    width: 70px;
    height: 60px;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
}

/* About section */
.btn-book-detail-1 {
    padding: 8px 15px;
    background-color: var(--secondr-color);
    border: none;
    border-radius: 5px;
    font-size: 1.1rem;
    font-weight: bold;
    color: #fbf6e8;
    ;
    width: auto;
    border: 1px solid var(--secondr-color);
    transition: 0.3s ease-in-out;
}

#box-2 {
    display: flex;
    justify-content: center;
    flex-direction: column;
    background-image: url(/images/luggage.png), url(/images/resaurant/banner-5.png);
    background-repeat: no-repeat;
    background-position: center;
    background-size: contain;
}

#box-2 h6 {
    font-family: var(--second-font-family);
    font-size: 2.5rem;
    margin-bottom: -10px;
    text-transform: capitalize;
}

#box-2 h1 {
    font-size: 3.5rem;
    color: var(--secondr-color);
}

.btn-book-detail-1:hover {
    background-color: transparent;
    color: var(--secondr-color);
}

#box-1 img {
    border-radius: 8px;
}

/* footer */
.footer {
    background: #9b804e;
    padding: 30px 0px;
    font-family: 'Play', sans-serif;
    text-align: center;
}

.footer .row {
    width: 100%;
    margin: 1% 0%;
    padding: 0.6% 0%;
    color: rgb(255, 255, 255);
    font-size: 0.8em;
}

.footer .row a {
    text-decoration: none;
    color: rgb(255, 255, 255);
    transition: 0.5s;
}

.footer .row a:hover {
    color: #fff;
}

.footer .row ul {
    width: 100%;
}

.footer .row ul li {
    display: inline-block;
    margin: 0px 30px;
}

.footer .row-2 a i {
    font-size: 2em;
    margin: 0% 1%;
}

@media (max-width:720px) {
    .footer {
        text-align: left;
        padding: 5%;
    }

    .footer .row ul li {
        display: block;
        margin: 10px 0px;
        text-align: left;
    }

    .footer .row a i {
        margin: 0% 3%;
    }
}

#footer-icon {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: 20px;
}

#footer-icon a {
    color: rgb(255, 255, 255);
}



/*     <---------HOTEL SECTION----------->     */
/*  <-----our Specail Room's----->  */
#best-room {
    border-radius: 20px;
    padding: 15px 0px;
    width: 100%;
    gap: 10px;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;

}

.card-room:hover .card-body-room {
    right: 0;

}

.card-room {
    max-width: 26rem;
    max-height: 18rem;
    border-radius: 5px;
    overflow: hidden;
    position: relative;
    margin-top: 0.254rem;
}

.card-img-room {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.card-body-room {
    width: 100%;
    height: 100%;
    top: 0;
    right: -100%;
    position: absolute;
    backdrop-filter: blur(2px);
    color: #fff;
    padding: 20px;
    display: flex;
    transition: 0.5s ease-in-out;
    flex-direction: column;
    justify-content: flex-end;
}

.card-title-room {
    text-transform: capitalize;
    font-size: 1.9rem;
    font-weight: 500;
}

.card-footer-room {
    text-transform: capitalize;
    font-size: 1.3rem;
}

/*  <-----our Offers Room's----->  */
.swiper {
    width: 100%;
    height: auto;
    padding: 50px 0;
}

.swiper-slide {
    text-align: center;
    font-size: 18px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.swiper-slide img {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.swiper-button-next,
.swiper-button-prev {
    color: #fbf6e8;
    padding: 4px 12px;
    border-radius: 100%;

}

.swiper-button-next:after,
.swiper-button-prev:after {
    font-size: 1.5rem;
    font-weight: bold;

}

.card-body {
    display: flex;
    flex-direction: column;
    justify-content: center;
    background-color: #9b804e;
    color: #fbf6e8;
    align-items: center;
    padding: 20px;
}

.card-body h6 {
    font-size: 0.6rem;
    letter-spacing: 4px;
    font-weight: bold;
    color: #fbf6e8;
    text-align: center;
    margin-bottom: 1rem;
}

.card-body h5 {
    font-size: 1.4rem;
    color: #fbf6e8;
    text-align: center;
    margin-bottom: 1rem;
}

.card-body p {
    text-align: center;
    font-size: 1rem;
}

.btn-card-simple {
    background-color: #fbf6e8;
    outline: none;
    text-transform: uppercase;
    text-decoration: none;
    border: none;
    padding: 5px 15px;
    color: #9b804e;
    transition: all 0.4s ease-in-out;
    border: 1px solid #fbf6e8;
}

.btn-card-simple:hover {
    background-color: transparent;
    color: #fbf6e8;
}

#best-offer {
    background-color: #fbf6e8;
}


